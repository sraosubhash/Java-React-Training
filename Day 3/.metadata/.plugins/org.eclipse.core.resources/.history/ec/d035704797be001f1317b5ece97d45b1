package com.java.project;

public class EmployMain {
	
	static EmployOps empOps;
	
	static {
		empOps = new EmployOps();
	}
	
	public static void readEmpMain() {
		empOps.readEmp();
	}
	
	public static void showEmpMain() {
		empOps.showEmploy();
	}
	
	public static void main(String[] args) {
		readEmpMain();
		showEmpMain();
	}

}
